{
	"name": "@zemio/web",
	"version": "0.3.0-alpha.1",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "next build",
		"auth:generate": "pnpm dlx @better-auth/cli generate --config ./src/server/better-auth/server.ts",
		"db:generate": "prisma migrate dev && prisma generate",
		"db:migrate": "prisma migrate deploy",
		"db:migrate:prod": "dotenv -e .env.prod -- prisma migrate deploy",
		"db:push": "prisma db push",
		"db:studio": "prisma studio",
		"dev": "next dev --turbo",
		"postinstall": "prisma generate",
		"preview": "SKIP_ENV_VALIDATION=1 next build && next start",
		"start": "next start",
		"typecheck": "tsc --noEmit",
		"email:preview": "dotenv -e .env.prod -- email dev --dir ./src/components/emails --port 3030"
	},
