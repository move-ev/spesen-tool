{
	"name": "spesen-tool",
	"version": "0.2.0-alpha.1",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "SKIP_ENV_VALIDATION=1 next build",
		"check": "biome check .",
		"auth:generate": "pnpm dlx @better-auth/cli generate --config ./src/server/better-auth/server.ts",
		"check:unsafe": "biome check --write --unsafe .",
		"check:write": "biome check --write .",
		"config:validate": "tsx scripts/validate-config.ts",
		"db:generate": "prisma migrate dev && prisma generate",
		"db:migrate": "prisma migrate deploy",
		"db:migrate:prod": "dotenv -e .env.prod -- prisma migrate deploy",
		"db:push": "prisma db push",
		"db:studio": "prisma studio",
		"dev": "next dev --turbo",
		"postinstall": "prisma generate",
		"preview": "SKIP_ENV_VALIDATION=1 next build && next start",
		"start": "next start",
		"typecheck": "tsc --noEmit",
		"prepare": "husky",
		"email:preview": "dotenv -e .env -- email dev --dir ./src/components/emails --port 3030"
	},
	"dependencies": {
		"@auth/prisma-adapter": "^2.7.2",
		"@base-ui/react": "^1.1.0",
		"@better-upload/client": "^3.0.12",
		"@better-upload/server": "^3.0.12",
		"@prisma/adapter-pg": "^7.2.0",
		"@prisma/client": "^7.2.0",
		"@react-email/components": "1.0.4",
		"@t3-oss/env-nextjs": "^0.12.0",
		"@tanstack/react-form": "^1.27.7",
		"@tanstack/react-query": "^5.69.0",
		"@trpc/client": "^11.0.0",
		"@trpc/react-query": "^11.0.0",
		"@trpc/server": "^11.0.0",
		"better-auth": "^1.3",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"dotenv": "^17.2.3",
		"lucide-react": "^0.562.0",
		"next": "^16.1.3",
		"next-themes": "^0.4.6",
		"pdfkit": "^0.17.2",
		"pg": "^8.17.1",
		"react": "^19.2.3",
		"react-day-picker": "^9.13.0",
		"react-dom": "^19.2.3",
		"react-dropzone": "^14.3.8",
		"resend": "^6.7.0",
		"server-only": "^0.0.1",
		"sonner": "^2.0.7",
		"superjson": "^2.2.1",
		"tailwind-merge": "^3.4.0",
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.5",
		"tsx": "^4.19.0",
		"@commitlint/cli": "^20.3.1",
		"@commitlint/config-conventional": "^20.3.1",
		"@react-email/preview-server": "5.2.5",
		"@tailwindcss/postcss": "^4.0.15",
		"@types/node": "^20.14.10",
		"@types/pdfkit": "^0.17.4",
		"@types/pg": "^8.16.0",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"dotenv-cli": "^11.0.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"postcss": "^8.5.3",
		"prisma": "^7.2.0",
		"react-email": "5.2.5",
		"tailwindcss": "^4.0.15",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.8.2"
	},
	"ct3aMetadata": {
		"initVersion": "7.40.0"
	},
	"packageManager": "pnpm@9.15.5"
}
